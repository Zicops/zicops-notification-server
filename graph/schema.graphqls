input NotificationInput {
  title: String!
  body: String!
}

type Notification {
  statuscode: String!
}

type FirestoreMessage {
  title: String!
  body: String!
  created_at: Int!
  user_id: String!
  prevPageSnapShot: String
}

type Mutation {
  sendNotification(notification: NotificationInput!): Notification!
}

type Query {
  getAll(prevPageSnapShot: String!, pageSize: Int!): [FirestoreMessage!]!
}